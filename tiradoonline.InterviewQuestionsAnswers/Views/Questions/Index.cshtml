@model IEnumerable<tiradoonline.DataAccess.InterviewQuestionsAnswers.Models.Questions>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Subjects.Subject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Question)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.create_dt)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Subjects.Subject)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Question)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.create_dt)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.QuestionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.QuestionID }) |
